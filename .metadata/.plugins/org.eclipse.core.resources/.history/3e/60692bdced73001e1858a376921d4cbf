package entity;

public class DonGiaoHang {
   private String maDonGiaoHang;
   private String tenKhachHang;
   private String sdt;
   private String diaChi;
   private String soKg;
   private boolean trangThai;
   private float soKm;
   private NhanVien nhanVien;
public DonGiaoHang(String maDonGiaoHang, String tenKhachHang, String sdt, String diaChi, String soKg, boolean trangThai,
		float soKm, NhanVien nhanVien) {
	super();
	this.maDonGiaoHang = maDonGiaoHang;
	this.tenKhachHang = tenKhachHang;
	this.sdt = sdt;
	this.diaChi = diaChi;
	this.soKg = soKg;
	this.trangThai = trangThai;
	this.soKm = soKm;
	this.nhanVien = nhanVien;
}
   
   
   
   
}
